{
    "name": "leet-count",
    "description": "Tracks number of unique Leetcode questions solved per day",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["https://leetcode.com/*"],
            "js": ["scripts/content.js"],
            "run_at": "document_idle"
        }
    ]
}